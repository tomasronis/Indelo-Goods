{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/.well-known/*",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ],
  "navigationFallback": {
    "rewrite": "/404.html",
    "exclude": ["/_next/*", "/api/*", "/.well-known/*", "/images/*", "/*.{css,js,json,ico,png,jpg,jpeg,gif,svg,woff,woff2,ttf,eot}"]
  },
  "platform": {
    "apiRuntime": "node:18"
  }
}
